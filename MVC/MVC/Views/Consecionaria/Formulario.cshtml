@model MVC.Models.Consecionarias
<!doctype html>
<html>
@{
    Layout = null;
}
<head>
<table class="table">
   <head>
    <title>CONSECIONARIA</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
   </head>
</table>    
        <!-- CSS only -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
</head>



<body>

    <h1>AUTO</h1>
    <hr />
    <form>
       <div class="container text-center">
           <div class="row">
                <div class="col-12 col-md-6"><h3>ID</h3>
                <input type="text" class="form-control"value="@Model.ID"></div>
                <div class="col-12 col-md-6"><h3>MARCA</h3>
                <input type="text" class="form-control"value="@Model.Marca"></div>
                <div class="col-12 col-md-6"><h3>MODELO</h3>
                <input type="text" class="form-control"value="@Model.Modelo"></div>
                <div class="col-12 col-md-6"><h3>AÑO</h3>
                <input type="text" class="form-control"value="@Model.Año"></div>            
                <br />          
            <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                <button type="button"class="btn btn-primary">ENVIAR</button>
                <a href="/consecionaria" button type="button"class="btn btn-primary">VOLVER</a>                   
            </div>
           </div> 
       </div>
    </form>
  
   

    
    <!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
</body>
</html>


