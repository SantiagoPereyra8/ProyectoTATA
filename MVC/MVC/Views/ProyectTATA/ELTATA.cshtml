@model MVC.Models.ProyectTata
@{
    Layout = null;
}
<!doctype html>
<html>
    <head>
        <title>EL TATA</title>
    



        <!-- CSS only -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
<link rel="stylesheet" href="~/assets/css/MENUTATA.css"/>
    </head>
    <body>

        <div class="head">
            <div class="logo">
                <img src="~/assets/img/rotiseria.jpg"  height=" 60px"; width="60px"/>            
            </div>          
        </div>  
        
        <header class="content header">           
            <div class="btn-home">                              
            </div> 
            <a><img src="~/assets/img/rotiseria.jpg" width="200px"height="200px";align="center";/></a>
        </header>
      
        <form class="form">

            <div class="container text-center">
                <div class="row">                    
                     <div class="col-12 col-md-6"><h3>ID</h3>
                        <input type="text" class="form-control"value="@Model.id" disabled></div>
                     <div class="col-12 col-md-6"><h3>PRODUCTO</h3>
                        <input type="text" class="form-control"value="@Model.producto" disabled></div>                   
                     <div class="col-12 col-md-6"><h3>DETALLE</h3>
                        <input type="text" class="form-control"value="@Model.detalle" required></div>                       
                     <div class="col-12 col-md-6"><h3>PRECIO</h3>
                        <input type="text" class="form-control"value="$@Model.precio" disabled></div>                
                 <br />          
                 <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                     <a href="/proyecttata" button type="button"class="btn btn-primary">VOLVER</a> 
                     <a href="/proyecttata/gracias" button type="button"class="btn btn-primary">ENVIAR</a>                 
                 </div>                   
               </div>
            </div>
        </form>

        
        



    






    <footer>

    </footer>
        <!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
    </body>    
</html>
